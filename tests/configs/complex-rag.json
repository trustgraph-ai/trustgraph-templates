{
    "llm": {
        "engine": "openai",
        "model": "gpt-4",
        "temperature": 0.7
    },
    "embeddings": {
        "engine": "hf",
        "model": "sentence-transformers/all-MiniLM-L6-v2"
    },
    "vector_store": {
        "engine": "qdrant"
    },
    "graph_store": {
        "engine": "neo4j"
    },
    "document_store": {
        "engine": "cassandra"
    },
    "rag": {
        "enabled": true,
        "chunking": "recursive"
    },
    "prompt": {
        "style": "openai"
    }
}